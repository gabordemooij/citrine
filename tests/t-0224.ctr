
>> countries := List new
	~ ['Switzerland']
	~ ['Italy']
	~ ['Germany'].

>> foods := List new
	~ ['Rösti']
	~ ['Spaghetti']
	~ ['Sauerkraut'].


>> counter := 0.
foods by: countries, each: { :key :value
	counter add: 1.
}.

Out write: (['we serve counter dishes'] counter: counter), stop.

